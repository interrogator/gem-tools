		<segment id="s-1.15" xref="u-1.15">City of Helsinki</segment>
		<segment id="s-1.16" xref="u-1.16">Tourist &amp; Convention Bureau</segment>
		<segment id="s-1.17" xref="u-1.17">Pohjoisesplanadi 19, P.O. Box 28, FI-00099 City of Helsinki</segment>
		<segment id="s-1.18" xref="u-1.18.1">Tel. +358 (0)9 3101 3300, Fax, +358 (0)9 3101 3301,</segment>
		<segment id="s-1.19" xref="u-1.18.2">tourist.info@hel.fi</segment>
		<segment id="s-1.20" xref="u-1.19">Open: 2.5-30.9: Mon-Fri 9am-8pm, Sat-Sun 9am-6pm</segment>
		<segment id="s-1.21" xref="u-1.20">1.10-1.5: Mon-Fri 9am-8pm, Sat-Sun 10am-4pm</segment>
